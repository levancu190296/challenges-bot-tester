<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="NopCommerce">
  	
	<test name="run on Firefox">
	<parameter name="browser" value="firefox" ></parameter>
		<classes>
			<class name="bot.conduit.user.Test_01_Challenges"/>
		</classes>
  	</test>	 
  	
	<test name="run on headless Firefox">
	<parameter name="browser" value="h_firefox" ></parameter>
		<classes>
			<class name="bot.conduit.user.Test_01_Challenges"/>
		</classes>
  	</test>	 
  	
	<test name="run on  Chrome">
	<parameter name="browser" value="chrome" ></parameter>
		<classes>
			<class name="bot.conduit.user.Test_01_Challenges"/>
		</classes>
  	</test>	 
  	
	<test name="run on  headless chrome">
	<parameter name="browser" value="h_chrome" ></parameter>
		<classes>
			<class name="bot.conduit.user.Test_01_Challenges"/>
		</classes>
  	</test>	 
  	
	<test name="run on edge">
	<parameter name="browser" value="edge" ></parameter>
		<classes>
			<class name="bot.conduit.user.Test_01_Challenges"/>
		</classes>
  	</test>	 

</suite >